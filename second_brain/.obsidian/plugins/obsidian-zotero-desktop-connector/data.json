{
  "database": "Zotero",
  "noteImportFolder": "",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "IEEE",
      "format": "pandoc"
    }
  ],
  "exportFormats": [
    {
      "name": "Literature Source Note",
      "outputPathTemplate": "sources/@{{citekey}}.md",
      "imageOutputPathTemplate": "sources/attachments/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "templates/source.md",
      "cslStyle": "ieee"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "pdfExportImageTessDataDir": "zotero/tesseract"
}